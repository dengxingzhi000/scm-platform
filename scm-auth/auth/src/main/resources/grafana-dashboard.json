{
    "dashboard": {
    "title": "业务监控大盘",
        "panels": [
        {
            "title": "登录成功率",
            "targets": [
                {
                    "expr": "rate(business_login_attempts_total{success=\"true\"}[5m]) / rate(business_login_attempts_total[5m])"
                }
            ]
        },
        {
            "title": "API响应时间P95",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket[5m]))"
                }
            ]
        },
        {
            "title": "JVM内存使用率",
            "targets": [
                {
                    "expr": "jvm_memory_used_bytes / jvm_memory_max_bytes * 100"
                }
            ]
        }
    ]
}
}